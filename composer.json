{
    "name": "sigma-net/php-sdk",
    "description": "sigma.net PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|~8.0.0|~8.1.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0|^2.0|^3.0",
        "guzzlehttp/psr7": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "guzzlehttp/guzzle": "^6.0.0|^7.0.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Guzzle Api Transport"
    },
    "autoload": {
        "psr-4": {
            "SigmaNet\\SDK\\": "src/",
            "Tests\\SigmaNet\\SDK\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8.x-dev"
        }
    }
}
